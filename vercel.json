{
    "headers": [
        {
            "source": "/installer/(.*)",
            "headers": [
                {
                    "key": "Content-Disposition",
                    "value": "attachment; filename=$1"
                },
                {
                    "key": "Content-Type",
                    "value": "application/octet-stream"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}